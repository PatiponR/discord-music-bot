{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start:uat": "cross-env NODE_ENV=uat node --loader ts-node/esm src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=uat nodemon --exec node --loader ts-node/esm src/index.ts"
  },
  "dependencies": {
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}